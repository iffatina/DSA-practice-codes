#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n; cin>>n;
    int roll[n];
    for(int i=0;i<n;i++)
    {
        cin>>roll[i];
    }
    cout << "Before deleting Firt element from array : " << endl;
    for(int i=0;i<n;i++)
    {
        cout<<roll[i]<< " ";
    }
    cout<<endl;

     cout << "After deleting "<<1<<"th element from array : " << endl;

             for(int i=0;i<n;i++)
    {
        roll[i]=roll[i+1];
    }
     for(int i=0;i<n-1;i++)
    {
        cout<<roll[i]<< " ";
    }
    cout<<endl;


   return 0;
}
